<?xml version="1.0" encoding="UTF-8"?>
<ContentPage Title="Settings" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="BudgetBadger.Forms.Settings.SettingsPage">
	<ContentPage.Content>
        <TableView Intent="Settings">
            <TableRoot>
                <TableSection>
                    <TextCell Text="Sync" Detail="{Binding SyncMode}" Command="{Binding SyncCommand}"/>
                </TableSection>
            </TableRoot>
        </TableView>
	</ContentPage.Content>
</ContentPage>
